    "header": {
      "allOf": [
        {
          "$ref": "/v2-header"
        },
        {
          "type": "object",
          "required": [
            "authentication"
          ],
          "properties": {
            "name": {
              "type": "string",
              "enum": [
                "ChangeReport"
              ]
            }
          }
        }
      ]
    },
    "endpoint": {
      "allOf": [
        {
          "$ref": "/v2-endpoint"
        },
        {
          "type": "object",
          "required": [
            "endpointId",
            "states"
          ]
        }
      ]
    },
    "payload": {
      "type": "object",
      "properties": {
        "change": {
          "type": "object",
          "properties": {
            "cuase": {
              "type": "string",
              "enum": [
                "PHYSICAL_INTERACTION"
              ]
            }
          }
        }
      }
    }
  }
}
